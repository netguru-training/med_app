%h1= 'All Entries'

%table.table.table-striped
  %tr
    %th #
    %th Examination
    %th Date
    %th Value
    %th Last examination
  =paginate decorated_entry_collection, :theme => 'twitter-bootstrap-3'
  - id = 1
  - decorated_entry_collection.each do |entry|
    %tr
      %td= id
      %td= entry.name
      %td= entry.my_date
      %td= entry.value_with_unit
      %td= entry.date_ago
    - id += 1
